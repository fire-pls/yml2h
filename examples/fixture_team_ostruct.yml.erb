# A team "fixture", using ERB+ivars to get dynamically-generated values to ouput a ruby OpenStruct
--- !ruby/object:OpenStruct
table:
  :name: <%= @name %>
  :rank: <%= @rank %>
  :users: <% @users.each do |user| %>
    - :name: <%= user[:name] %>
      :email: <%= user[:email] %>
      :tos_acceptance: <%= user[:tos_acceptance] %>
      :address:
        :street: <%= user[:address][:street] %>
      :profile:
        :street: <%= user[:profile][:introduction] %>
        :hobbies: <% user[:profile][:hobbies].each do |hobby| %>
          - <%= hobby %>
        <% end %>
      :meta:
        :created_at: <%= user[:meta][:created_at] %>
        :updated_at: <%= user[:meta][:updated_at] %>
        :uuid: <%= user[:meta][:uuid] %>
  <% end %>
  :budget:
    :amount: <%= @budget[:amount] %>
    :meta:
      :created_at: <%= @budget[:meta][:created_at] %>
      :updated_at: <%= @budget[:meta][:updated_at] %>
      :uuid: <%= @budget[:meta][:uuid] %>
  :meta:
    :created_at: <%= @meta[:created_at] %>
    :updated_at: <%= @meta[:updated_at] %>
    :uuid: <%= @meta[:uuid] %>
